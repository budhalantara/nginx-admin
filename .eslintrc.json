{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "space-before-function-paren": ["error", {
      "named": "never",
      "anonymous": "never"
    }],
    "camelcase": ["error",
      {
        "ignoreDestructuring": true,
        "properties": "never"
      }
    ]
  }
}
